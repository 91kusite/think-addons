{
    "name": "91kusite/think-addons",
    "description": "addons package for 91kusite",
    "type": "library",
    "require": {
        "php": ">=7.1.9",
        "topthink/think-helper": ">=1.0.4",
        "topthink/think-installer": "^2.0",
        "topthink/framework": "5.1.*"
    },
    "license": "Apache-2.0",
    "authors": [{
        "name": "xiaosun",
        "email": "syh@sunyonghong.com"
    }],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "think\\": "src/"
        },
        "files": ["src/common.php"]
    },
    "extra": {
        "think-config": {
            "addons": "src/config.php"
        }
    }
}